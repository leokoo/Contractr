<center>
<div>
	<%= form_for(@job) do |f| %>
		<div>
		<%= f.label :name %>
		<%= f.text_field :name%>
		</div>
		<div>
		<%= f.label :pay_offer %>
		<%= f.text_field :pay_offer%>
		</div>

		<% @job_skills.all.each do |jobskill| %>
			<%= jobskill.skill %><br>
		<% end %>

		<div>
		<%= f.submit :'Create Job', class: "btn btn-default" %>
		</div>
	<% end %>
</div>
</center>